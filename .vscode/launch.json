{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Transactions",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "transactions-build",
            "program": "${workspaceFolder}/apps/transactions/src/main.ts",
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "cwd": "${workspaceFolder}/apps/transactions",
            "console": "integratedTerminal"
        }
    ]
}